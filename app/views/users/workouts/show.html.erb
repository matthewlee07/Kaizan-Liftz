<div class="container-fluid main pb-5 mb-5">
    <div class="col-sm-10 devise-form rounded bg-dark text-warning my-4 container">
        <div class="accordion" id="accordionWorkoutEntries">
            <%= render "details" %>
            
        <div class="card bg-dark text-light">
            <div class="card-header" id="headingTwo">
                <h5 class="mb-0 text-center bg-secondary">

                    <button class="btn btn-lg btn-link text-warning collapsed" type="button" data-toggle="collapse" data-target="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo">
                    Workout Entries
                    </button>
                </h5>
            </div>
            <div id="collapseTwo" class="collapse" aria-labelledby="headingTwo" data-parent="#accordionWorkoutEntries">
                <div class="card-body">
                    <%= render partial: "workout_entry", collection: @workout_entries %>
                </div>
            </div>
        </div>
            <%# <%= render "users/workout_entries/new" %>
            
            
            <div class="text-center mt-5">
                <%= link_to 'New Workout Entry', new_user_workout_workout_entry_path(current_user, @workout), class: "btn btn-outline-warning"  %>
                <%= link_to 'Back', user_workouts_path, class: "btn btn-outline-warning"  %>
            </div>
        </div>
    <div>
</div>
